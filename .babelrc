{
  "env": {
    "development": {
      "plugins": ["@babel/plugin-proposal-class-properties"],
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs",
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "sourceMaps": "inline",
      "retainLines": true
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs",
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
